{
  "name": "mesh-tree",
  "version": "0.4.1",
  "description": "Utility functions for traversing the Medical Subject Heading (MeSH) tree",
  "main": "index.js",
  "scripts": {
    "initdb-test": "tar -xzf test/fixtures/mesh_subset.nt.tar.gz -C test/fixtures; node_modules/levelgraph-n3/import.js test/fixtures/mesh_subset.nt -o ./dbtest; rm test/fixtures/mesh_subset.nt",
    "test": "NODE_ENV=test PATH_TO_MESH_TESTDB=dbtest node_modules/.bin/mocha --reporter spec --compilers js:babel/register",
    "initdb": "node_modules/levelgraph-n3/import.js $PATH_TO_MESH_RDF -o $PATH_TO_MESH_DB",
    "initdb-quiet": "node_modules/levelgraph-n3/import.js --quiet $PATH_TO_MESH_RDF -o $PATH_TO_MESH_DB",
    "coverage": "rm -rf coverage; NODE_ENV=test PATH_TO_MESH_TESTDB=dbtest node_modules/.bin/babel-node -r node_modules/.bin/isparta cover _mocha --report lcovonly -- --reporter spec --compilers js:babel/register --bail",
    "publish-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "server": "node ./util/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/standard-analytics/mesh-tree.git"
  },
  "keywords": [
    "MeSH",
    "NLM",
    "PubMed",
    "Medical",
    "Subject",
    "Headings",
    "RDF"
  ],
  "author": "Sebastien Ballesteros",
  "contributors": [
    "Sebastien Ballesteros <sebastien@standardanalytics.io>",
    "Leon Chen <leon@standardanalytics.io>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/standard-analytics/mesh-tree/issues"
  },
  "devDependencies": {
    "babel": "^5.8.5",
    "chai": "^3.2.0",
    "coveralls": "^2.11.3",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "cheerio": "^0.19.0",
    "co": "^4.6.0",
    "expand-home-dir": "0.0.2",
    "level": "^1.2.0",
    "levelgraph": "^1.0.0",
    "levelgraph-n3": "^1.0.0",
    "lodash": "^3.10.0",
    "prettyjson": "^1.1.2",
    "progress-stream": "^1.1.1",
    "request": "^2.60.0"
  }
}
