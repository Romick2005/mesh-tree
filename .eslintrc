{
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "no-eval": 2,
    "no-console": 0,
    "no-undef": 0,
    "no-unused-vars": 0
  },
  "env": {
    "mocha": true,
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "extends": "eslint:recommended"
}
